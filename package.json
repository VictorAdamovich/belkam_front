    "@types/node": "^12.12.21",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
